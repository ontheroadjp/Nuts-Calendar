{
    "require": {
        "php": ">=5.5.9",
        "tymon/jwt-auth": "0.5.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.2.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Nuts\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "vendor/orchestra/testbench/src/TestCase.php"
        ],
        "psr-4": {
            "Nuts\\Api\\Test\\": "tests/"
        }
    }
}
